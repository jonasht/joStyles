/* QPuhshButton =-=-=-=-=-=-=-=-=-=-=-=-=-=-= */
QPushButton {
    color: #ffffff;
    background-color: #555555;
    border: 1.5px solid #555555;
    border-radius: 8px;
    font-size: 16px;
    padding: 7px 22px;
    font-weight: 500;
}
QPushButton:hover {
    background-color: #767676;
}
QPushButton:pressed {
    background-color: #2e2e2e;
}

QPushButton#primary {
    background-color: #4ea1ff;
    border: 1.5px solid #4ea1ff;
}
QPushButton#primary:hover {
    background-color: #85b9ff;
}
QPushButton#primary:pressed {
    background-color: #2675ff;
}

QPushButton#secondary {
    background-color: #555555;
    border: 1.5px solid #555555;
}
QPushButton#secondary:hover {
    background-color: #767676;
}
QPushButton#secondary:pressed {
    background-color: #2e2e2e;
}

QPushButton#success {
    background-color: #77b300;
    border: 1.5px solid #77b300;
}
QPushButton#success:hover {
    background-color: #a4d155;
}
QPushButton#success:pressed {
    background-color: #4f6d00;
}

QPushButton#info {
    background-color: #9933cc;
    border: 1.5px solid #9933cc;
}
QPushButton#info:hover {
    background-color: #c08ff3;
}
QPushButton#info:pressed {
    background-color: #661d99;
}

QPushButton#warning {
    background-color: #ff8800;
    border: 1.5px solid #ff8800;
}
QPushButton#warning:hover {
    background-color: #ffae33;
}
QPushButton#warning:pressed {
    background-color: #cc6600;
}

QPushButton#danger {
    background-color: #cc0000;
    border: 1.5px solid #cc0000;
}
QPushButton#danger:hover {
    background-color: #e16d6d;
}
QPushButton#danger:pressed {
    background-color: #8b0000;
}

QPushButton#link {
    background: transparent;
    border: none;
    color: #4ea1ff;
    font-size: 15px;
    text-align: left;
    padding: 0;
    font-weight: 500;
    text-decoration: none;
}
QPushButton#link:hover {
    color: #9933cc;
    text-decoration: underline;
}
QPushButton#link:pressed {
    color: #ff8800;
}
/* QPushButton disabled */
QPushButton#primary:disabled {
    background-color: #7aaeff;
    color: #e0e0e0;
    border-color: #7aaeff;
}
QPushButton#secondary:disabled {
    background-color: #6e6e6e;
    color: #cccccc;
    border-color: #6e6e6e;
}
QPushButton#success:disabled {
    background-color: #90c344;
