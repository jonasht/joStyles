}
QTextEdit#info:read-only {
    border: 2px solid #9933cc;
    background-color: transparent;
}
QTextEdit#warning:read-only {
    border: 2px solid #ff8800;
    background-color: transparent;
}
QTextEdit#danger:read-only {
    border: 2px solid #cc0000;
    background-color: transparent;
}

/* QTextEdit fill read-only --- */
