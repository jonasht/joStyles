QTextEdit {
    background-color: transparent;
    color:  #ffffff;
    border: 1px solid #454545;
    border-radius: 8px;
    font-size: 15px;
    padding: 6px 10px;
}
QTextEdit:focus {
    border: 2px solid #454545;
    color: #ffffff;
    background-color: transparent;
}

QTextEdit#primary {
    border: 1px solid #4ea1ff;
}
QTextEdit#primary:focus {
    border: 2px solid #4ea1ff;
    background-color: transparent;
}

QTextEdit#secondary {
    border: 1px solid #555555;
}
QTextEdit#secondary:focus {
    border: 2px solid #555555;
    background-color: transparent;
}
QTextEdit#success {
    border: 1px solid #77b300;
}
QTextEdit#success:focus {
    border: 2px solid #77b300;
    background-color: transparent;
}
QTextEdit#info {
    border: 1px solid #9933cc;
}
QTextEdit#info:focus {
    border: 2px solid #9933cc;
    background-color: transparent;
}
QTextEdit#warning {
    border: 1px solid #ff8800;
}
QTextEdit#warning:focus {
    border: 2px solid #ff8800;
    background-color: transparent;
}
QTextEdit#danger {
    border: 1px solid #cc0000;
}
QTextEdit#danger:focus {
    border: 2px solid #cc0000;
    background-color: transparent;
}
/* QTextEdit normal disabled */
QTextEdit:disabled {
    background-color: #454545;
    color:  #ffffff;
}
QTextEdit#primary:disabled {
    background-color: #454545;
    border: 1px solid #4ea1ff;

}

QTextEdit#secondary:disabled {
    background-color: #454545;
    border: 1px solid #555555;

}
QTextEdit#success:disabled {
    background-color: #454545;
}
QTextEdit#info:disabled {
    background-color:#454545;
    border: 1px solid #9933cc;
}
QTextEdit#warning:disabled {
    background-color:#454545;
    border: 1px solid #ff8800;
}
QTextEdit#danger:disabled {
    background-color:#454545;
    border: 1px solid #cc0000;
}

/* ------ fill ---------------------- */
QTextEdit#primary-fill {
    color: #ffffff;
    border: 1px solid #4ea1ff;
    background-color: #4ea1ff;
}
QTextEdit#primary-fill:hover {
    border: 1px solid #85b9ff;
    background-color: #4ea1ff;
}
QTextEdit#primary-fill:focus {
    border: 2px solid #4ea1ff;
    background-color: #2675ff;
}

QTextEdit#secondary-fill {
    color: #ffffff;
    border: 1px solid #555555;

}
