QLabel {
    font-size: 15px;
    font-weight: 500;
    background-color: transparent;
}
QLabel#primary {
    color: #4ea1ff;
    border: none !important;
}

QLabel#secondary {
    color: #555555;
    border: none !important;
}
QLabel#success {
    color: #77b300;
    border: none !important;
}
QLabel#info {
    color: #9933cc;
    border: none !important;
}

QLabel#warning {
    color: #ff8800;
    border: none !important;
}
QLabel#danger {
    color: #cc0000;
    border: none !important;
}

/* label inverse */
QLabel#primary-inverse {
    color: #ffffff;
    background-color: #4ea1ff;
