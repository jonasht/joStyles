QFrame { 
    border-radius: 8px;
    border-radius: 8px;
    background-color: #191919;
}
/* Frame Primary */
QFrame#primary {
    border-radius: 8px;
    border: 2px solid #4ea1ff;
    background-color: #191919;
}
QFrame#secondary {
    border-radius: 8px;
    border: 2px solid #555555;
    background-color: #191919;
}
/* Frame Success */
QFrame#success {
    border-radius: 8px;
    border: 2px solid #77b300;
    background-color: #191919;
}
/* Frame Info */
QFrame#info {
    border-radius: 8px;
    border: 2px solid #9933cc;
    background-color: #191919;
} 


/* Frame Warning */
QFrame#warning {
    border: 2px solid #ff8800;
    border-radius: 8px;
    background-color: #191919;
}

/* Frame Danger */
QFrame#danger {
    border: 2px solid #cc0000;
    border-radius: 8px;
    background-color: #191919;

}
/* frame fill ---------------------------------- */
QFrame QLabel {
    background-color: transparent;
    color: #ffffff;
}
QFrame#primary-fill {
    border-radius: 8px;
    background: #4ea1ff;
}


QFrame#secondary-fill {
    border-radius: 8px;
    background: #555555;
}
QFrame#success-fill {
    border-radius: 8px;
    background: #77b300;
}
QFrame#info-fill {
    border-radius: 8px;
    background-color: #9933cc;
}
QFrame#warning-fill {
    border-radius: 8px;
    background-color: #ff8800;
}
QFrame#danger-fill {
    border-radius: 8px;
    background-color: #cc0000;
}


