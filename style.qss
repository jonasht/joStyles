/* --- QLineEdit --- --------------------- */

QLineEdit {
    background-color: transparent;
    color: #ffffff;
    border: 1px solid #454545;
    border-radius: 8px;
    font-size: 15px;
    padding: 6px 10px;
}
QLineEdit:focus {
    border: 2px solid #454545;
}
/* ------------------ */
QLineEdit#primary {
    border: 1px solid #4ea1ff;
    background-color: @transparent;
    color: #ffffff;
}
QLineEdit#primary:focus {
    border: 2px solid #4ea1ff;
}

QLineEdit#secondary {
    border: 1px solid #555555;
    background-color: transparent;
    color: #ffffff;
}
QLineEdit#secondary:focus {
    border: 2px solid #555555;
}

QLineEdit#success {
    border: 1px solid #77b300;
    background-color: transparent;
    color: #ffffff;
}
QLineEdit#success:focus {
    border: 2px solid #77b300;
}
QLineEdit#info {
    border: 1px solid #9933cc;
    background-color: transparent;
    color: #ffffff;
}
QLineEdit#info:focus {
    border: 2px solid #9933cc;
}

QLineEdit#warning {
    border: 1px solid #ff8800;
    background-color: transparent;
    color: #ffffff;
}
QLineEdit#warning:focus {
    border: 2px solid #ff8800;
}
QLineEdit#danger {
    border: 1px solid #cc0000;
    background-color: transparent;
    color: #ffffff;
}
QLineEdit#danger:focus {
    border: 2px solid #cc0000;
}

