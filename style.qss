/* TABLEWIDGET BASE */

QTableWidget {
    background-color: #1c1c1c;       /* fundo geral */
    color: #ffffff;                  /* texto normal */
    border: 1px solid #333;
    gridline-color: #333;
    selection-background-color: #4ea1ff; /* azul primary */
    selection-color: #ffffff;
    alternate-background-color: #222;
    border-radius: 4px;
}

/* CABE√áALHO HORIZONTAL */

QHeaderView::section {
    background-color: #4ea1ff;    /* azul primary */
    color: #ffffff;
    padding: 4px;
    border: 1px solid #1c1c1c;
    font-weight: bold;
}

/* CABE√áALHO VERTICAL */

QHeaderView::section:vertical {
    background-color: #4ea1ff;
    color: #ffffff;
    padding: 4px;
    border: 1px solid #1c1c1c;
}

/* CANTINHO SUPERIOR ESQUERDO */

QTableCornerButton::section {
    background-color: #4ea1ff;
    border: 1px solid #1c1c1c;
}

/* ITEM SELECIONADO - REMOVER BORDA DE FOCO */

QTableWidget::item:selected:active {
    outline: none;
}


