    border-color: #cc6600;
}

QCheckBox#danger-border::indicator {
    border-color: #cc0000;
    
}
QCheckBox#danger-border::indicator:hover {
    border-color: #e16d6d;
}
QCheckBox#danger-border::indicator:checked {
    background-color: #cc0000;
    border-color: #8b0000;
}
