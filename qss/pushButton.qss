/* QPuhshButton =-=-=-=-=-=-=-=-=-=-=-=-=-=-= */
QPushButton {
    color: @fg;
    background-color: @secondary;
    border: 1.5px solid @secondary;
    border-radius: 8px;
    font-size: 16px;
    padding: 7px 22px;
    font-weight: 500;
}
QPushButton:hover {
    background-color: @secondarylight;
}
QPushButton:pressed {
    background-color: @secondarydark;
}

QPushButton#primary {
    background-color: @primary;
    border: 1.5px solid @primary;
}
QPushButton#primary:hover {
    background-color: @primarylight;
}
QPushButton#primary:pressed {
    background-color: @primarydark;
}
QPushButton#primary:checked {
    background-color: @primarydark;
}

QPushButton#secondary {
    background-color: @secondary;
    border: 1.5px solid @secondary;
}
QPushButton#secondary:hover {
    background-color: @secondarylight;
}
QPushButton#secondary:pressed {
    background-color: @secondarydark;
}
QPushButton#secondary:checked {
    background-color: @secondarydark;
}

QPushButton#success {
    background-color: @success;
    border: 1.5px solid @success;
}
QPushButton#success:hover {
    background-color: @successlight;
}
QPushButton#success:pressed {
    background-color: @successdark;
}
QPushButton#success:checked {
    background-color: @successdark;
}
QPushButton#info {
    background-color: @info;
    border: 1.5px solid @info;
}
QPushButton#info:hover {
    background-color: @infolight;
}
QPushButton#info:pressed {
    background-color: @infodark;
}
QPushButton#info:checked {
    background-color: @infodark;
}
QPushButton#warning {
    background-color: @warning;
    border: 1.5px solid @warning;
}
QPushButton#warning:hover {
    background-color: @warninglight;
}
QPushButton#warning:pressed {
    background-color: @warningdark;
}
QPushButton#warning:checked {
    background-color: @warningdark;
}
QPushButton#danger {
    background-color: @danger;
    border: 1.5px solid @danger;
}
QPushButton#danger:hover {
    background-color: @dangerlight;
}
QPushButton#danger:pressed {
    background-color: @dangerdark;
}
QPushButton#danger:checked {
    background-color: @dangerdark;
}
QPushButton#link {
    background: transparent;
    border: none;
    color: @primary;
    font-size: 15px;
    text-align: left;
    padding: 0;
    font-weight: 500;
    text-decoration: none;
}
QPushButton#link:hover {
    color: @info;
    text-decoration: underline;
}
QPushButton#link:pressed {
    color: @warning;
}
QPushButton#link:checked {
    color: @warning;
}
/* QPushButton disabled */
QPushButton#primary:disabled {
    background-color: @primarydis;
    color: @fgprimarydis;
    border-color: @primarydis;
}
QPushButton#secondary:disabled {
    background-color: #6e6e6e;
    color: #cccccc;
    border-color: #6e6e6e;
}
QPushButton#success:disabled {
    background-color: #90c344;
    color: #e0e0e0;
    border-color: #90c344;
}
QPushButton#info:disabled {
    background-color: #b07cd9;
    color: #e0e0e0;
    border-color: #b07cd9;
}
QPushButton#warning:disabled {
    background-color: #e69933;
    color: #333333;
    border-color: #e69933;
}
QPushButton#danger:disabled {
    background-color: #d38080;
    color: #e0e0e0;
    border-color: #d38080;
}


